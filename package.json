  "main": "server/server.js",
  "scripts": {
    "analyze": "source-map-explorer 'dist/static/js/*.js'",
    "start": "GENERATE_SOURCEMAP=false react-scripts start",
    "start:server": "NODE_EXTRA_CA_CERTS=server/cert/lets-encrypt-r3.pem nodemon server/server.js",
    "restart": "",
    "build": "react-scripts build && rm -rf dist && mv build dist",
    "eslint": "eslint --ext js,ts,tsx src",
    "eslint:fix": "eslint --fix --ext js,ts,tsx src",
    "typecheck": "tsc",
    "prettier:fix": "prettier --write \"{server,src}/**/*.{js,ts,tsx}\"",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "translate": "node scripts/translate.js",
    "serve": "pm2 start server/server.js"
  },
  "proxy": "http://localhost:3010",
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
